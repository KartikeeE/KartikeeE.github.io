{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
    "text": "Rate of Unemplyment in India from 1992 to 2022",
    "anchor": "start",
    "subtitle": "Rate, % | Source: CSO (WB) through ECO API",
    "fontSize": "20",
    "subtitleFontSize": "13",
    "color": "#000000",
    "subtitleColor": "#000000"
  },
  "height": "400",
  "width": "600",
  "autosize": {
    "type": "fit",
    "contains": "padding"
  },
  "background": "#ffffff",
  "view": {
    "fill": "#ffffff",
    "strokeOpacity": 0
  },
  "data": {
    "url": "https://api.economicsobservatory.com/ind/unem?vega",
    "format": {
      "type": "json"
    }
  },
  "mark": {
    "type": "line",
    "color": "#15b1f4",
    "strokeWidth": "2"
  },
  "encoding": {
    "x": {
      "field": "date",
      "type": "temporal",
      "axis": {
        "title": "Years",
        "titleColor": "#122B39",
        "grid": false
      }
    },
    "y": {
      "field": "value",
      "type": "quantitative",
      "axis": {
        "title": "Unemployment %",
        "titleColor": "#122B39",
        "grid": false
      }
    }
  }
}